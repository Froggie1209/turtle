import turtle
import random

screen=turtle.Screen()
me = turtle.Turtle()
me.shape("turtle")
me.speed(9999999999999999999999999999999999999)
me.ht()




def letters(me,letter,startx = 0, starty = 0):



  if letter == "A":
    me.lt(70)
    me.fd(100)
    me.rt(140)
    me.fd(100)
    me.rt(180)
    me.fd(50)
    me.lt(70)
    me.fd(35)
    me.hideturtle() 
    me.penup()
    me.sety(0)

  elif letter == "B":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.pd()
    me.lt(270)
    me.fd(100)
    me.rt(270)
    me.fd(10)
    me.circle(25,180)
    me.fd(10)
    me.lt(180)
    me.fd(10)
    me.circle(25,180)
    me.fd(10)
    me.hideturtle() 
    me.penup() 
    me.sety(0)

  elif letter == "C":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.fd(50)
    me.pd()
    me.rt(200)
    for i in range(220):
      me.lt(1)
      me.fd(1)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
    
  elif letter == "D":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.pd()
    me.lt(270)
    me.fd(100)
    me.lt(90)
    me.fd(40)
    me.circle(50,180)
    me.fd(40)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "E":
    me.lt(90)
    me.fd(66)
    me.rt(90)
    me.fd(30)
    me.rt(180)
    me.fd(30)
    me.lt(90)
    me.fd(33.33333)
    me.lt(90)
    me.fd(30)
    me.rt(180)
    me.fd(30)
    me.lt(90)
    me.fd(33.33333)
    me.lt(90)
    me.fd(30)
    me.rt(180)
    me.fd(30)
    me.rt(90)
    me.hideturtle() 
    me.penup() 
    me.sety(0)

  elif letter == "F":
    me.lt(90)
    me.fd(66)
    me.rt(90)
    me.fd(30)
    me.rt(180)
    me.fd(30)
    me.lt(90)
    me.fd(33.33333)
    me.lt(90)
    me.fd(30)
    me.rt(180)
    me.fd(30)
    me.lt(90)
    me.hideturtle() 
    me.penup()
    me.sety(0)

  elif letter == "G":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.fd(50)
    me.pd()
    me.rt(200)
    for i in range(200):
      me.lt(1)
      me.fd(1)
    me.fd(20)
    me.lt(90)
    me.fd(50)
    me.lt(90)
    me.fd(30)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
    

  elif letter == "H":
    me.lt(90)
    me.fd(100)
    me.rt(180)
    me.fd(50)
    me.lt(90)
    me.fd(50)
    me.lt(90)
    me.fd(50)
    me.rt(180)
    me.fd(100)
    me.hideturtle() 
    me.penup() 
    me.sety(0)

  elif letter == "I":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.pd()
    me.fd(50)
    me.rt(180)
    me.fd(25)
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.fd(25)
    me.rt(180)
    me.fd(50)
    me.hideturtle() 
    me.penup() 
    me.sety(0)

  elif letter == "J":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.pd()
    me.rt(90)
    me.fd(75)
    me.rt(180)
    me.circle(25,-180)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "K":
    me.lt(90)
    me.fd(100)
    me.rt(180)
    me.fd(50)
    me.rt(270)
    me.lt(45)
    me.fd(65)
    me.rt(180)
    me.fd(65)
    me.rt(180)
    me.rt(90)
    me.fd(65)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "L":
    me.fd(50)
    me.backward(50)
    me.lt(90)
    me.fd(100)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
    
  elif letter == "M":
    me.penup()
    me.fd(20)
    me.pd()
    me.lt(90)
    me.fd(100)
    me.rt(135)
    me.fd(50)
    me.lt(90)
    me.fd(50)
    me.rt(135)
    me.fd(100)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
    
  elif letter == "M":
    me.lt(90)
    me.fd(100)
    me.rt(135)
    me.fd(50)
    me.lt(90)
    me.fd(50)
    me.rt(135)
    me.fd(100)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "N":
    me.lt(90)
    me.fd(100)
    me.rt(153.43)
    me.fd(111.8034)
    me.lt(153.43)
    me.fd(100)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "O":
    me.penup()
    me.lt(90)
    me.fd(30)
    me.rt(90)
    me.fd(30)
    me.pd()
    me.lt(90)
    me.fd(30)
    me.circle(25,180)
    me.fd(30)
    me.circle(25,180)
    me.penup()
    me.rt(90)
    me.fd(50)

    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "P":
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.fd(25)
    me.rt(180)
    me.circle(25,-180)
    me.backward(25)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "Q":
    me.lt(50)
    me.circle(50,360)
    me.lt(90)
    me.fd(25)
    me.backward(50)
    me.hideturtle() 
    me.penup() 
    me.sety(0)

  elif letter == "R":
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.fd(25)
    me.rt(180)
    me.circle(25,-180)
    me.backward(25)
    me.rt(45)
    me.fd(70.710678118655)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "S":
    me.fd(25)
    me.circle(25,180)
    me.rt(180)
    me.circle(25,-180)
    me.backward(25)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "T":
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.fd(25)
    me.backward(50)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "U":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.pd()
    me.rt(90)
    me.fd(75)
    me.circle(25,180)
    me.fd(75)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "V":
    me.penup()
    me.fd(20)
    me.pd()
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.pd()
    me.rt(180)
    me.lt(70)
    me.fd(100)
    me.rt(140)
    me.fd(100)
    me.rt(180)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "W":
    me.penup()
    me.lt(90)
    me.fd(100)
    me.rt(90)
    me.fd(75)
    me.pd()
    me.rt(180)
    me.lt(80)
    me.fd(100)
    me.rt(125)
    me.fd(50)
    me.lt(90)
    me.fd(50)
    me.rt(125)
    me.fd(100)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "X":
    me.penup()
    me.fd(20)
    me.pd()
    me.rt(-45)
    me.fd(120)
    me.rt(180)
    me.fd(60)
    me.rt(90)
    me.fd(60)
    me.fd(-120)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "Y":
    me.rt(-90)
    me.fd(40)
    me.rt(30)
    me.fd(35)
    me.fd(-35)
    me.rt(-60)
    me.fd(35)
    me.hideturtle() 
    me.penup() 
    me.sety(0)
  elif letter == "Z":
    me.fd(40)
    
    me.fd(-40)
    me.rt(-45)
    me.fd(60)
    me.rt(-135)
    me.fd(40)
    me.hideturtle() 
    me.penup() 
    me.sety(0)

letter = input("say some text and i will write it(caps only)")
listofletters = list(letter)
me.penup()
me.setposition(-1000,0)
me.pd()
for letter in listofletters:
  me.penup()
  me.setheading(0)
  me.fd(100)
  me.pd()
  print(letter)
  me.ht()
  letters(me,letter)
